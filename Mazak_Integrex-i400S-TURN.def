  }

  ADDRESS Y 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      [$mom_sys_leader(Y)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS Z 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      [$mom_sys_leader(Z)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS I 
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "I"
      ZERO_FORMAT Zero_real
  }

  ADDRESS J 
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "J"
      ZERO_FORMAT Zero_real
  }

  ADDRESS K 
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "K"
      ZERO_FORMAT Zero_real
  }

  ADDRESS F 
  {
      FORMAT      Feed
      FORCE       off
      MAX         9999.9 Warning
      MIN         .0001 Warning
      LEADER      "F"
  }

  ADDRESS S 
  {
      FORMAT      Digit_5
      FORCE       off
      MAX         4000 Truncate
      MIN         0 Truncate
      LEADER      "S"
      ZERO_FORMAT Zero_int
  }

  ADDRESS T 
  {
      FORMAT      Tcode
      FORCE       off
      MAX         999 Truncate
      MIN         0 Truncate
      LEADER      "T"
  }

  ADDRESS D 
  {
      FORMAT      Digit_3
      FORCE       off
      MAX         999 Truncate
      MIN         0 Truncate
      LEADER      "D"
  }

  ADDRESS D_1 
  {
      FORMAT      Digit_3
      FORCE       off
      MAX         999 Truncate
      MIN         0 Truncate
      LEADER      ""
  }

  ADDRESS For_tool_type 
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         999 Truncate
      MIN         0 Truncate
      LEADER      ""
  }

  ADDRESS B_preselect 
  {
      FORMAT      Tcode
      FORCE       off
      MAX         999 Truncate
      MIN         0 Truncate
      LEADER      " B"
  }

  ADDRESS dwell 
  {
      FORMAT      Dwell_SECONDS
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "X"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_dwell 
  {
      FORMAT      Dwell_SECONDS
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_real
  }

  ADDRESS P_cutcom 
  {
      FORMAT      Dwell_SECONDS
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_real
  }

  ADDRESS K_cycle 
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "K"
